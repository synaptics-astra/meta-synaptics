From: Xiaomei Shang <xiaomei.shang@synaptics.com>
Date: Tue, 16 Jan 2024 18:58:19 +0800
Subject: [PATCH] set fxdiv/fp16/pthreadpool/cpuinfo dir

---
 CMakeLists.txt | 5 +++++
 1 file changed, 5 insertions(+)

diff --git a/CMakeLists.txt b/CMakeLists.txt
index 0d165a90a..7272a536c 100755
--- a/CMakeLists.txt
+++ b/CMakeLists.txt
@@ -141,6 +141,11 @@ ENDIF()
 
 # ---[ Download deps
 IF(NOT XNNPACK_USE_SYSTEM_LIBS)
+  set(CPUINFO_SOURCE_DIR ${CMAKE_CURRENT_LIST_DIR}/../cpuinfo)
+  set(FP16_SOURCE_DIR ${CMAKE_CURRENT_LIST_DIR}/../fp16)
+  set(FXDIV_SOURCE_DIR ${CMAKE_CURRENT_LIST_DIR}/../fxdiv)
+  set(PTHREADPOOL_SOURCE_DIR ${CMAKE_CURRENT_LIST_DIR}/../pthreadpool)
+
   IF(NOT DEFINED CPUINFO_SOURCE_DIR)
     MESSAGE(STATUS "Downloading cpuinfo to ${CMAKE_BINARY_DIR}/cpuinfo-source (define CPUINFO_SOURCE_DIR to avoid it)")
     CONFIGURE_FILE(cmake/DownloadCpuinfo.cmake "${CMAKE_BINARY_DIR}/cpuinfo-download/CMakeLists.txt")
